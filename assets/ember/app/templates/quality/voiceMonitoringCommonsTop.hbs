<div class="box box-abacus">
    <div class="box-body">
        <div class="form-group col-sm-3 ">
            <div class="input-group input-group-sm">
                <div class="input-group-addon" style="border-color: #38b64d;"><i class="fa">Monitoring</i></div>
                {{view "select" class="form-control input-sm"  content=monitoredBy  id="forQualityMonitor" optionValuePath="content.id" optionLabelPath="content.name" style="border-left: 0px;" valueBinding="monitoring.monitoring_for_id"  prompt="Select monitoring to ..."}}
            </div>
        </div>
        <div class="form-group col-sm-3 ">
            <div class="input-group input-group-sm">
                <div class="input-group-addon" style="border-color: #38b64d;"><i class="fa">Monitoring To</i></div>
                {{view "select" class="form-control"  style="border-left: 0px;"   content=monitorBelons  id="belongQualityMonitor" optionValuePath="content.id" optionLabelPath="content.name" valueBinding="monitoring.monitoring_belongs"  prompt="Select Monitoring ..."}}
            </div>
        </div>
        <div class="form-group col-sm-3 ">
            <div class="input-group input-group-sm">
                <div class="input-group-addon" style="border-color: #38b64d;"><i class="fa">Ticket ID/Phone #</i></div>
                {{input type="number" class="form-control" style="border-left: 0px;" valueBinding="monitoring.ticket_no" id="ticketQualityMonitor" }}
            </div>
        </div>
        <div class="form-group col-sm-3 ">
            <div class="input-group input-group-sm">
                <div class="input-group-addon" style="border-color: #38b64d;"><i class="fa">Booking ID</i></div>
                {{input type="number" class="form-control" style="border-left: 0px;" valueBinding="monitoring.booking_no" id="bookingQualityMonitor" }}
            </div>
        </div>
