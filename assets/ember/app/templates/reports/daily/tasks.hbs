<div class="box box-abacus">
	<div class="box-header">
		<i class="fa fa-bar-chart-o pull-left"></i>
		<h3 class="box-title pull-left"> Agent Task Report</h3>
	</div>
	<div class="box-body">
		<div class="row">
			<div class="form-group form-group-sm col-md-4 col-sm-6 col-xs-12">
				<label for="typeEmailDriver">Campaign For</label>
				{{view "select" class="form-control input-sm"  content=model  id="typeEmailDriver" optionValuePath="content.id" optionLabelPath="content.name" valueBinding="report.monitoring_for_id"  prompt="Select Campaing ..."}}
			</div>
			{{!-- <div class="form-group form-group-sm col-md-4 col-sm-6 col-xs-12">
				<label for="typeEmailDriver">Ticket Process</label>
				{{view "select" class="form-control input-sm"  content=ticketProcess  id="ticketProcess" optionValuePath="content.id" optionLabelPath="content.name" valueBinding="report.ticket_process"  prompt="Select Ticket Process ..."}}
			</div> --}}
			<div class="form-group form-group-sm col-md-4 col-sm-6 col-xs-12">
				<label for="regions">Date</label>
				<div class="input-group input-group-sm">
					<div class="input-group-addon"><i class="fa fa-calendar"></i></div>
					{{input class="form-control pull-right" valueBinding="report.date" id="rptVoqDateAgent" }}
					<span class="input-group-btn">
						<button class="btn btn-flat btn-abacus"
							{{action "saveRptVoqDataDateWiseTableAgent" report}}>Download</button>
					</span>
				</div><!-- /.input group -->
			</div>
		</div>
	</div>
</div>


